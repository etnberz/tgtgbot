{
    "version": 1,
    "formatters": {
        "default": {
            "format": "%(asctime)s [%(process)d] in %(name)s [%(levelname)s]  %(message)s",
            "datefmt": "[%Y-%m-%d %H:%M:%S %z]"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "formatter": "default",
            "level": "INFO"
        }
    },
    "root": {"level": "INFO",  "handlers": ["console"]},
    "loggers": {
        "tgtgbot": {
            "level": "INFO",
            "propagate": true
        }
    }
}
